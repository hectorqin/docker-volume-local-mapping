        "Capabilities": null,
        "AllowAllDevices": false,
        "Devices": null
    },
    "Mounts": [{
        "destination": "/mnt/state",
        "options": [
            "rbind"
        ],
        "name": "state",
        "source": "/var/lib/docker/plugins",
        "settable": [
            "source"
        ],
        "type": "bind"
    }],
    "Network": {
        "Type": ""
